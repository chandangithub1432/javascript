//  write a factorial in javascript program using loops
const factorialOf = integer =>{
    let factorial = 1;
    for(let i = 1; i <= integer; i++ ){
        factorial *= i;
    }
    return factorial;
}
console.log(factorialOf(5));
console.log(factorialOf(2));
// Output= 120
//        = 2



